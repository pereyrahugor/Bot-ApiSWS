{
    "title": "API SWS - Documentación v5.0",
    "description": "Documentación oficial de la API del Sistema Web SWS",
    "author": "Equipo SWS",
    "language": "es",
    "gitbook": "3.x.x",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "search",
        "fontsettings",
        "sharing",
        "livereload",
        "code",
        "prism",
        "-highlight"
    ],
    "pluginsConfig": {
        "sharing": {
            "facebook": false,
            "twitter": false,
            "google": false,
            "weibo": false,
            "all": [
                "print"
            ]
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        }
    },
    "pdf": {
        "pageNumbers": true,
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        }
    }
}